{
  "name": "resonate-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "eslint:check": "eslint . --ext .jsx,.js,.ts,.tsx",
    "prettier:check": "prettier --check '**/*.{js,ts,jsx,tsx}'",
    "eslint:fix": "eslint --fix . --ext .jsx,.js,.ts,.tsx",
    "prettier:fix": "prettier --write '**/*.{js,ts,jsx,tsx}'",
    "lint": "npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-community/slider": "4.2.3",
    "@react-navigation/material-bottom-tabs": "^6.2.4",
    "@react-navigation/native": "^6.0.13",
    "expo": "^46.0.0",
    "expo-av": "~12.0.4",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "lodash": "^4.17.21",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-paper": "^4.12.5",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/lodash": "^4.14.184",
    "@types/react-native": "~0.69.1",
    "eslint": "^8.26.0",
    "eslint-config-universe": "^11.1.0",
    "husky": ">=7",
    "lint-staged": ">=10",
    "prettier": "1.12.1",
    "typescript": "^4.6.3"
  },
  "private": true,
  "resolutions": {
    "@types/react": "17.0.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
